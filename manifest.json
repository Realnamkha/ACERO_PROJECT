{
    "name": "ACERO",
    "version": "2.0",
    "manifest_version": 3,
    "description": "Acero_SEO - An extension to help you to improve you on page seo",
    "permissions": ["scripting",
    "activeTab",
    "storage",
    "tabs"],
    "host_permissions": [
      "<all_urls>"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_title": "ACERO",
      "default_popup": "popup.html",
      "default_icon": {
        "32": "/assets/logo3.png"
      }
    },
    "icons": {
      "16": "/assets/logo.svg",
      "32": "/assets/logo.svg",
      "48": "/assets/logo.svg",
      "128": "/assets/logo.svg"
    }
  }